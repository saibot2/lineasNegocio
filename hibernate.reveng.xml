<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="REAL" hibernate-type="java.lang.Float"
			precision="12" not-null="true">
		</sql-type>
	</type-mapping>
	<table catalog="pruebas" name="almacenes">
		<column name="id_almacen"></column>
		<column name="nombre_almacen"></column>
		<column name="telefono"></column>
		<column name="esactivo"></column>
	</table>
	<table catalog="pruebas" name="productos">
		<column name="id_producto"></column>
		<column name="nombre_producto"></column>
		<column name="pvp"></column>
		<column name="id_almacen"></column>
	</table>
	<table catalog="pruebas" name="naipes">
		<column name="id_naipe"></column>
		<column name="numero"></column>		
		<column name="palo"></column>
	</table>
</hibernate-reverse-engineering>